{
  "name": "ringcentral-web-phone",
  "version": "0.9.0",
  "scripts": {
    "test": "jest",
    "test:coverage": "cat .coverage/lcov.info | coveralls -v",
    "build": "yarn run build:tsc && yarn run build:webpack",
    "build:tsc": "tsc",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --color",
    "start": "webpack serve --config webpack.config.js --progress --color",
    "watch": "npm-run-all --print-label --parallel \"build:** -- --watch\"",
    "lint": "eslint . --cache --cache-location node_modules/.cache/eslint --fix",
    "lint:all": "yarn run lint \"src/**/*.ts\" \"demo/**/*.js\"",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "sip.js": "0.21.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@ringcentral/sdk": "4.7.3",
    "@types/express-serve-static-core": "4.17.31",
    "@types/jest": "29.2.3",
    "@types/jest-environment-puppeteer": "5.0.3",
    "@types/node": "18.11.9",
    "buffer": "6.0.3",
    "cache-loader": "4.1.0",
    "copy-webpack-plugin": "11.0.0",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "crypto-browserify": "3.12.0",
    "dotenv-override-true": "6.2.2",
    "eslint": "8.27.0",
    "eslint-config-ringcentral-typescript": "7.0.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "jest": "29.3.1",
    "jest-puppeteer": "6.1.1",
    "jquery": "3.6.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "puppeteer": "19.2.2",
    "querystring-es3": "0.2.1",
    "stream-browserify": "3.0.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "wait-for-async": "0.6.0",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "yarn-upgrade-all": "0.7.1"
  },
  "preferGlobal": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
